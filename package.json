{
  "name": "typescript-nextjs-starter",
  "description": "A TypeScript starter for Next.js that includes all you need to build amazing projects",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "packageManager": "bun@1.3.6",
  "author": "Jo√£o Pedro Schmitz (@jpedroschmitz)",
  "license": "MIT",
  "keywords": [
    "nextjs",
    "starter",
    "typescript"
  ],
  "scripts": {
    "dev": "__NEXT_NODE_NATIVE_TS_LOADER_ENABLED=true next dev",
    "build": "__NEXT_NODE_NATIVE_TS_LOADER_ENABLED=true next build --webpack",
    "build:turbo": "__NEXT_NODE_NATIVE_TS_LOADER_ENABLED=true next build --turbo",
    "start": "next start",
    "type-check": "SKIP_ENV_VALIDATION=true __NEXT_NODE_NATIVE_TS_LOADER_ENABLED=true next typegen && SKIP_ENV_VALIDATION=true tsc --noEmit",
    "build:analyze": "__NEXT_NODE_NATIVE_TS_LOADER_ENABLED=true next experimental-analyze --output",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:bootstrap-admin": "bun src/bin/bootstrap-admin.ts",
    "db:seed:dev": "bun src/bin/dev-seed.ts",
    "db:up": "docker compose -f docker-compose.local-db.yml up -d",
    "db:down": "docker compose -f docker-compose.local-db.yml down",
    "db:down:volumes": "docker compose -f docker-compose.local-db.yml down -v",
    "db:logs": "docker compose -f docker-compose.local-db.yml logs -f postgres",
    "db:reset": "bun src/bin/db-reset.ts",
    "db:reset:seed": "bun run db:reset && bun run db:seed:dev",
    "db:setup:docker": "bun run db:up && bun run db:reset:seed",
    "db:setup": "bun run db:migrate && bun run db:seed:dev",
    "db:studio": "prisma studio",
    "scheduler": "bun src/bin/scheduler.ts",
    "worker": "bun src/bin/worker.ts",
    "test": "vitest",
    "test:ci": "vitest run --coverage",
    "e2e": "playwright test",
    "lint": "eslint \"src/**/*.+(ts|js|tsx)\"",
    "lint:fix": "eslint \"src/**/*.+(ts|js|tsx)\" --fix",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "format:ci": "prettier --list-different .",
    "postinstall": "husky"
  },
  "dependencies": {
    "@alicloud/ecs20140526": "7.6.0",
    "@alicloud/openapi-client": "0.4.15",
    "@alicloud/rds20140815": "15.4.0",
    "@alicloud/tea-util": "1.4.11",
    "@asteasolutions/zod-to-openapi": "8.4.0",
    "@bokuweb/zstd-wasm": "0.0.27",
    "@hookform/resolvers": "5.2.2",
    "@prisma/client": "6.19.2",
    "@radix-ui/react-dialog": "1.1.15",
    "@radix-ui/react-dropdown-menu": "2.1.16",
    "@radix-ui/react-label": "2.1.8",
    "@radix-ui/react-select": "2.2.6",
    "@radix-ui/react-slot": "1.2.4",
    "@radix-ui/react-switch": "1.2.6",
    "@t3-oss/env-nextjs": "0.13.10",
    "@tanstack/react-table": "8.21.3",
    "ajv": "8.17.1",
    "ajv-formats": "3.0.1",
    "babel-plugin-react-compiler": "1.0.0",
    "bcryptjs": "3.0.3",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "fast-xml-parser": "5.3.3",
    "ldapts": "8.1.6",
    "lucide-react": "0.563.0",
    "next": "16.1.4",
    "next-themes": "0.4.6",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-hook-form": "7.71.1",
    "sonner": "2.0.7",
    "swagger-ui-react": "5.31.0",
    "tailwind-merge": "3.4.0",
    "tailwindcss-animate": "1.0.7",
    "winrm-client": "0.0.11",
    "zod": "4.3.6"
  },
  "devDependencies": {
    "@commitlint/cli": "20.3.1",
    "@commitlint/config-conventional": "20.3.1",
    "@eslint/js": "9.39.2",
    "@next/eslint-plugin-next": "16.1.4",
    "@playwright/test": "1.58.0",
    "@types/node": "25.0.10",
    "@types/react": "19.2.9",
    "@types/react-dom": "19.2.3",
    "@types/swagger-ui-react": "5.18.0",
    "@vitest/coverage-v8": "4.0.18",
    "autoprefixer": "10.4.23",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-import-x": "4.16.1",
    "eslint-plugin-prettier": "5.5.5",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.1",
    "globals": "17.0.0",
    "husky": "9.1.7",
    "lint-staged": "16.2.7",
    "postcss": "8.5.6",
    "prettier": "3.8.1",
    "prisma": "6.19.2",
    "tailwindcss": "3.4.17",
    "typescript": "5.9.3",
    "typescript-eslint": "8.53.1",
    "vitest": "4.0.18"
  }
}
